    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "yarn build"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "electron": "^1.7.10",
    "electron-builder": "^19.49.2",
    "foreman": "^2.0.0",
    "wait-on": "^2.0.2"
  },
  "build": {
    "appId": "com.bromc.electron-app",
    "files": [
      "build/**/*",
      "node_modules/**/*"
