  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "jsonwebtoken": "^8.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "semantic-ui-css": "^2.2.12",
